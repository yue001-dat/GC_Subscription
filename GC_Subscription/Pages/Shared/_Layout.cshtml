<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Ghostchef</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css" rel="stylesheet">
</head>
<body>
    <style>
        .overlay {
            position: fixed;
            width: 100%;
            height: 100%;
            background: rgba(0,0,0,0.75);
            visibility: hidden;
            z-index: 9999;
        }

        .spanner {
            position: absolute;
            top: 50%;
            left: 0;
            width: 100%;
            display: block;
            text-align: center;
            height: 300px;
        
            transform: translateY(-50%);
            z-index: 10000;
            visibility: hidden;
        }

        .show {
            visibility: visible;
        }

    </style>
    <div class="overlay"></div>
    <div class="spanner">
        <div class="lds-dual-ring"></div>
    </div>

    <div class="clearfix"><br /></div>
    <!-- Navbar-->
    <nav class="navbar navbar-expand-lg  justify-content-center">
        <div class="">
            <button class="navbar-toggler align-content-start" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarNav">
                <ul class="navbar-nav">
                    <li class="nav-item">
                        <span class="badge text-bg-danger weeks">UGE 23 | 25</span>
                        <a class="nav-link disabled" aria-current="page" href="#">Ugekassen</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link disabled" href="#">
                            <img src="https://ghostchef.dk/wp-content/uploads/2023/11/meal-svgrepo-com-1.svg" title="meal-svgrepo-com (1)" class="wd-nav-img" width="18" height="18">
                            Catering
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-page="/Buy/Index">
                            <img src="https://ghostchef.dk/wp-content/uploads/2023/10/box-food-takeaway-wok-svgrepo-com.svg" title="box-food-takeaway-wok-svgrepo-com" class="wd-nav-img" width="18" height="18">
                            Måltidskasser
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link disabled" aria-disabled="true">
                            <img loading="lazy" src="https://ghostchef.dk/wp-content/uploads/2023/11/knife-fork-svgrepo-com.svg" title="knife-fork-svgrepo-com" class="wd-nav-img" width="18" height="18">
                            Færdigretter
                        </a>
                    </li>
                    <a class="navbar-brand" asp-area="" asp-page="/Index">
                        <img src="https://ghostchef.dk/wp-content/uploads/2023/09/O_Logo_original_sort.png" alt="Ghostchef" class="logo">
                    </a>
                    <li class="nav-item">
                        <a class="nav-link disabled" aria-disabled="true">Take Away Holbæk</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link disabled" aria-disabled="true">Om Os</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link disabled" aria-disabled="true">Kontakt Os</a>
                    </li>
                    <li class="nav-item">
                        <a class="btn btn-primary" asp-area="" asp-page="/AdminDashboard/Mealboxes/Index">Gå til Admin Dashboard</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <!-- End Navbar-->

    <div class="container">
        <main role="main" class="pb-3">
            <div class="clearfix"><br /></div>
            @RenderBody()
        </main>
    </div>

    <div class="clearfix"><br /><br /></div>

    <footer class="border-top footer text-muted">
        <div class="container">
            <br/>
            &copy; 2024 - Ghostchef
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
    <!--<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>-->
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>